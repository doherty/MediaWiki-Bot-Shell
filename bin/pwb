#!/usr/bin/perl
use strict;
use warnings;

package
    pwb;
# ABSTRACT: a shell interface to your MediaWiki::Bot

use MediaWiki::Bot::Shell;
my $shell = MediaWiki::Bot::Shell->new();
$shell->cmdloop();

